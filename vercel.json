{
    "version":  2,
    "builds":  [
                   {
                       "src":  "api/**/*.js",
                       "use":  "@vercel/node"
                   }
               ],
    "routes":  [
                   {
                       "src":  "/api-docs",
                       "dest":  "/api/api-docs.js"
                   },
                   {
                       "src":  "/api/(?!.*\\.js$)(.*)",
                       "dest":  "/api/$1.js"
                   }
               ]
}
