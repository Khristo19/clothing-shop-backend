{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node",
            "config": {
                "maxDuration": 10
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/server.js"
        }
    ],
    "functions": {
        "api/**/*.js": {
            "maxDuration": 10
        },
        "server.js": {
            "maxDuration": 10
        }
    },
    "api": {
        "bodyParser": false
    }
}
